tFilename()&#10;outPath = filePath+fileName+&quot;_ChromatinCoordinates.csv&quot;&#10;&#10;writer = new FileWriter(outPath)&#10;writer.write(&quot;Edge ID, X (px), X (µm), Y (px), Y (µm), Z(slice), Z (µm), Mean width (µm), Length (µm), Intensity, Width (µm)\n&quot;)&#10;&#10;im = workspace.getImage(&quot;RawForCSV&quot;)&#10;ipl = null&#10;if (im != null) {&#10;&#9;ipl = im.getImagePlus()&#10;}&#10;&#10;widIm = workspace.getImage(&quot;Distance&quot;)&#10;widIpl = null&#10;if (widIm != null) {&#10;    widIpl = widIm.getImagePlus()&#10;}&#10;&#10;edges = workspace.getObjects(&quot;Edges&quot;)&#10;for (edge:edges.values()) {&#10;    id = String.valueOf(edge.getID())&#10;    width = String.valueOf(edge.getMeasurement(&quot;WIDTH_(µm)&quot;).getValue())&#10;    length = String.valueOf(edge.getMeasurement(&quot;SKELETON // LENGTH_(µm)&quot;).getValue())&#10;&#10;    dppXY = edge.getDppXY()&#10;    dppZ = edge.getDppZ()&#10;&#10;    for (pt:edge.getCoordinateSet()) {&#10;&#9;&#9;row = id + &quot;,&quot;&#10;        row = row + String.valueOf(pt.x) + &quot;,&quot; + String.valueOf(pt.x*dppXY) + &quot;,&quot;&#10;        row = row + String.valueOf(pt.y) + &quot;,&quot; + String.valueOf(pt.y*dppXY) + &quot;,&quot;&#10;        row = row + String.valueOf(pt.z) + &quot;,&quot; + String.valueOf(pt.z*dppZ) + &quot;,&quot;&#10;        row = row + String.valueOf(width) + &quot;,&quot; + String.valueOf(length) &#10;&#9;&#9;if (ipl != null) {&#10;&#9;&#9;&#9;ipl.setPosition(1, pt.getZ() + 1, edge.getT() + 1)&#10;&#9;&#9;&#9;row = row + &quot;,&quot; + String.valueOf(ipl.getProcessor().getf(pt.getX(), pt.getY()))&#10;&#9;&#9;}&#10;        if (widIm != null) {&#10;&#9;&#9;&#9;widIpl.setPosition(1, pt.getZ() + 1, edge.getT() + 1)&#10;&#9;&#9;&#9;row = row + &quot;,&quot; + String.valueOf(widIpl.getProcessor().getf(pt.getX(), pt.getY())*2*dppXY)&#10;&#9;&#9;}&#10;        row = row + &quot;\n&quot;&#10;&#10;        writer.write(row)&#10;&#10;    }&#10;}&#10;&#10;writer.close();&#10;" VIS