EXT="false" VALUE="Groovy" VISIBLE="false"/><PARAMETER NAME="Script text" NICKNAME="Script text" VALUE="// The following two parameters will provide references to the workspace and current module.&#10;#@ io.github.mianalysis.mia.object.Workspace workspace&#10;#@ io.github.mianalysis.mia.module.Module thisModule&#10;&#10;import io.github.mianalysis.mia.MIA&#10;import io.github.mianalysis.mia.module.Module&#10;import io.github.mianalysis.mia.module.objects.measure.intensity.MeasureObjectIntensity&#10;import io.github.mianalysis.mia.module.images.process.binary.DistanceMap&#10;import io.github.mianalysis.mia.module.images.process.InvertIntensity&#10;import io.github.mianalysis.mia.module.objects.process.CreateDistanceBands&#10;import io.github.mianalysis.mia.object.Objs&#10;import io.github.mianalysis.mia.object.measurements.Measurement&#10;&#10;inputObjectsName = &quot;Nucleosomes&quot;&#10;outputObjectsName = &quot;NucleosomeDistanceBands&quot;&#10;densityImageName = &quot;Density&quot;&#10;relativeMode = CreateDistanceBands.RelativeModes.OBJECT_CENTROID&#10;bandMode = CreateDistanceBands.BandModes.INSIDE_AND_OUTSIDE&#10;weightMode = CreateDistanceBands.WeightModes.W13_18_22_29_31&#10;matchZToXY = true&#10;bandWidth = NucBandWidth&#10;applyMinDist = false&#10;minDist = 0&#10;applyMaxDist = true&#10;maxDist = NucMaxDist&#10;type = CreateDistanceBands.VolumeTypes.QUADTREE&#10;&#10;inputObjects = workspace.getObjects(inputObjectsName)&#10;bandObjects = new Objs(outputObjectsName, inputObjects)&#10;densityImage = workspace.getImage(densityImageName)&#10;&#10;dppXY = inputObjects.getDppXY()&#10;bandWidth = bandWidth / dppXY&#10;minDist = minDist / dppXY&#10;maxDist = maxDist / dppXY&#10;&#10;if (!applyMinDist)&#10;    minDist = 0&#10;if (!applyMaxDist)&#10;    maxDist = Double.MAX_VALUE&#10;&#10;count = 0&#10;total = inputObjects.size()&#10;for (inputObject:inputObjects.values()) {&#10;    inputBorderWidths = CreateDistanceBands.getBorderWidths(inputObject, bandMode, applyMaxDist, maxDist)&#10;    referenceObject = CreateDistanceBands.getReferenceVolume(inputObject, relativeMode, false, false, &quot;&quot;)&#10;    extents = referenceObject.getExtents(true, false)&#10;    referenceBorderWidths = CreateDistanceBands.getBorderWidths(referenceObject, bandMode, applyMaxDist, maxDist)&#10;&#10;    inputImage = referenceObject.getAsTightImage(&quot;Binary&quot;, referenceBorderWidths)&#10;    InvertIntensity.process(inputImage)&#10;    maskImage = inputObject.getAsTightImage(&quot;Mask&quot;, inputBorderWidths)&#10;    &#10;    tempBandObjects = CreateDistanceBands.getAllBands(inputImage, maskImage, outputObjectsName, weightMode, matchZToXY, bandWidth, minDist, maxDist, type)&#10;    &#10;    // Transferring new band objects to main collection&#10;    for (tempBandObject : tempBandObjects.values()) {&#10;        // Update spatial calibration, so translated coordinates aren't out of range&#10;        tempBandObject.setSpatialCalibration(inputObject.getSpatialCalibration().duplicate())&#10;&#10;        // Shifting back to original coordinates&#10;        xShift = (int) Math.round(extents[0][0] - referenceBorderWidths[0][0])&#10;        yShift = (int) Math.round(extents[1][0] - referenceBorderWidths[1][0])&#10;        zShift = (int) Math.round(extents[2][0] - referenceBorderWidths[2][0])&#10;        tempBandObject.translateCoords(xShift, yShift, zShift)&#10;&#10;        tempBandObject.setID(bandObjects.getAndIncrementID())&#10;        for (measurement : tempBandObject.getMeasurements().values())&#10;            tempBandObject.addMeasurement(new Measurement(measurement.getName(), measurement.getValue()));&#10;&#10;        // Measuring intensity&#10;        MeasureObjectIntensity.measureIntensity(tempBandObject,densityImage,false,true)&#10;&#10;        // Removing coordinates&#10;        tempBandObject.clearAllCoordinates()&#10;&#10;        tempBandObject.setT(inputObject.getT())&#10;&#10;        tempBandObject.addParent(inputObject)&#10;        inputObject.addChild(tempBandObject)&#10;&#10;        bandObjects.add(tempBandObject)&#10;&#10;    }&#10;&#10;    Module.writeProgressStatus(++count, total, &quot;objects&quot;, &quot;Distance bands&quot;);&#10;&#10;}&#10;&#10;workspace.addObjects(bandObjects)&#10;" VISIBLE="false"/><PARAMETER NAME="Script file" NICKNAME="Script file" SHOW_TEXT="false" VALUE="/Users/sc13967/OneDrive - University of Bristol/People/Alice Sherrard/2025-04-14 DistanceBandsTemp.groovy" VISIBLE="false"/><PARAMETER NAME="Add parameter" NICKNAME="Add parameter" VALUE="" VISIBLE="false"><COLLECTION><PARAMS><PARAM NAME="Parameter type" NICKNAME="Parameter type" SHOW_TEXT="false" VALUE="Number" VISIBLE="false"/><PARAM NAME="Parameter name" NICKNAME="Parameter name" VALUE="NucBandWidth" VISIBLE="false"/><PARAM NAME="Parameter checkbox" NICKNAME="Parameter checkbox" SHOW_TEXT="false" VALUE="true" VISIBLE="false"/><PARAM NAME="Parameter numeric value" NICKNAME="Band width (µm)" VALUE="0.002" VISIBLE="true"/><PARAM NAME="Parameter text value" NICKNAME="Parameter text value" VALUE="" VISIBLE="false"/></PARAMS><PARAMS><PARAM NAME="Parameter type" NICKNAME="Parameter type" SHOW_TEXT="false" VALUE="Number" VISIBLE="false"/><PARAM NAME="Parameter name" NICKNAME="Parameter name" VALUE="NucMaxDist" VISIBLE="false"/><PARAM NAME="Parameter checkbox" NICKNAME="Parameter checkbox" SHOW_TEXT="false" VALUE="true" VISIBLE="false"/><PARAM NAME="Parameter numeric value" NICKNAME="Max. distance (µm)" VALUE="0.02" VISIBLE="true"/><PARAM NAME="Parameter text value" NICKNAME="Parameter text value" VALUE="" VISIBLE="false"/></PARAMS></COLLECTION></PARAMETER><PARAMETER NAME="Add output" NICKNAME="Add output" VALUE="" VISIBLE="false"><COLLECTION><PARAMS><PARAM NAME="Output type" NICKNAME="Output type" SHOW_TEXT="false" VALUE="Objects" VISIBLE="false"/><PARAM NAME="Output image" NICKNAME="Output image" VALUE="" VISIBLE="false"/><PARAM NAME="Output objects" NICKNAME="Output objects" VALUE="NucleosomeDistanceBands" VISI